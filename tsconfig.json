    "compilerOptions": {
        "module": "commonjs",
        "target": "es5",
        "outDir": "out",
        "lib": [
            "es5"
        ],
        "rootDir": "src",
        "strict": false,
        "noUnusedLocals": false,
        "noImplicitAny": true,
        "removeComments": true,
        "sourceMap": true,
    },
    "exclude": [
